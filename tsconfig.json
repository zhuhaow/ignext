{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "types": ["node", "jest"],
      "strict": true,
      "esModuleInterop": true,
      "allowJs": true,
      "noEmit": true
    },
    "include": [
      "**/*.ts",
      "**/*.tsx"
    ],
    "exclude": [
      "node_modules",
      "test/integration/**/pages/*",
    ]
}