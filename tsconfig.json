{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "lib": [
      "ES2020",
    ],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "types": [
      "@cloudflare/workers-types",
      "jest"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "test/integration/**/pages/*",
  ]
}