{
    "env": {
        "es2021": true
    },
    "extends": "xo",
    "overrides": [
        {
            "extends": [
                "xo-typescript/space"
            ],
            "files": [
                "*.ts",
                "*.tsx"
            ]
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {},
    "ignorePatterns": [
        "**/node_modules/**",
        "**/dist/**",
        "test/integration/**/pages/*",
        "test/integration/**/next-env.d.ts"
    ]
}